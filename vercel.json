{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "https://xyzcompany.supabase.co",
    "VITE_SUPABASE_ANON_KEY": "sua_chave_anonima_do_supabase",
    "VITE_APP_NAME": "Bueiro Digital",
    "VITE_APP_URL": "https://bueiro-digital.vercel.app"
  }
} 